{
	"name": "svg-2font-icons",
	"dependencies": {
		"rimraf": "5.0.7",
		"serve": "14.2.3",
		"svgtofont": "4.2.1"
	},
	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"build": "rimraf ./font && svgtofont --sources ./svg --output ./font",
		"start": "serve"
	},
	"svgtofont": {
		"fontName": "svg-2font-icons",
		"classNamePrefix": "svg-2font",
		"css": {
			"fileName": "style"
		}
	},
	"files": [
		"font",
		"svg"
	]
}
