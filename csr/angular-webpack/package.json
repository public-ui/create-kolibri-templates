{
	"name": "angular-webpack",
	"version": "0.0.0",
	"license": "UNLICENSED",
	"scripts": {
		"clean": "git clean -f -d -X",
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"coverage": "cross-env NODE_ENV=test nyc mocha --exit",
		"e2e": "nightwatch",
		"format": "prettier --check src",
		"xlint": "tsc --noemit && eslint src",
		"prepare": "npm-run-all prepare:*",
		"prepare:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" public/assets --dot",
		"prepare:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" public/assets --dot",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map --no-hot",
		"start": "npm run serve -- --open",
		"test": "cross-env NODE_ENV=test mocha --exit",
		"unused": "knip"
	},
	"dependencies": {
		"@angular/common": "18.0.3",
		"@angular/compiler": "18.0.1",
		"@angular/compiler-cli": "18.0.1",
		"@angular/core": "18.0.1",
		"@angular/forms": "18.0.3",
		"@angular/platform-browser": "18.0.3",
		"@angular/platform-browser-dynamic": "18.0.3",
		"@public-ui/angular-v17": "2.1.2",
		"@public-ui/components": "2.1.2",
		"@public-ui/themes": "2.1.2",
		"zone.js": "0.14.7"
	},
	"devDependencies": {
		"@leanup/stack": "1.3.54",
		"@leanup/stack-angular": "1.3.54",
		"@leanup/stack-webpack": "1.3.54",
		"@ngtools/webpack": "18.0.2",
		"@unocss/preset-mini": "0.60.4",
		"@unocss/webpack": "0.60.4",
		"chromedriver": "125.0.3",
		"cpy-cli": "5.0.0",
		"cross-env": "7.0.3",
		"knip": "5.20.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"nightwatch-axe-verbose": "2.3.1",
		"npm-check-updates": "16.14.20",
		"npm-run-all": "4.1.5",
		"rimraf": "5.0.7",
		"typescript": "5.4.5"
	},
	"files": [
		"dist"
	]
}
