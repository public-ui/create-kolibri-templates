{
	"name": "@public-ui/library-app",
	"version": "0.0.0",
	"license": "UNLICENSED",
	"scripts": {
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"build:deps": "pnpm --filter @public-ui/library-app^... build",
		"format": "prettier --check src",
		"lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
		"start": "npm run serve -- --open",
		"unused": "ts-prune -e src",
		"prepare": "npm-run-all prepare:*",
		"prepare:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" public/assets --dot",
		"prepare:themes-assets": "cpy \"node_modules/@public-ui/themes/assets/**/*\" public/assets --dot"
	},
	"dependencies": {
		"@leanup/stack": "1.3.53",
		"@leanup/stack-react": "1.3.53",
		"@leanup/stack-webpack": "1.3.53",
		"@public-ui/components": "2.1.1",
		"@public-ui/library-components": "workspace:*",
		"@public-ui/library-react": "workspace:*",
		"@public-ui/react": "2.1.1",
		"@types/node": "ts5.4",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@unocss/preset-uno": "0.58.9",
		"@unocss/webpack": "0.58.9",
		"ajv": "8.14.0",
		"chromedriver": "125.0.3",
		"cpy-cli": "5.0.0",
		"cross-env": "7.0.3",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.34.2",
		"formik": "2.4.6",
		"nightwatch-axe-verbose": "2.3.1",
		"npm-run-all": "4.1.5",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router": "6.23.1",
		"react-router-dom": "6.23.1",
		"rimraf": "5.0.7",
		"ts-prune": "0.10.3",
		"typescript": "5.4.5",
		"world_countries_lists": "2.9.0",
		"yup": "1.4.0"
	},
	"files": [
		".eslintignore",
		".eslintrc.js",
		".gitignore",
		".prettierignore",
		"dist/**",
		"prettier.config.js",
		"public/**",
		"src/**",
		"test-build.sh",
		"tsconfig.json",
		"unocss.config.ts",
		"webpack.config.js"
	]
}
